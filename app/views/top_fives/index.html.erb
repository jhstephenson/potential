<p id="notice"><%= notice %></p>

<h1>Listing Top Fives</h1>

<table>
  <thead>
    <tr>
      <th>Opportunity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @top_fives.each do |top_fife| %>
      <tr>
        <td><%= top_fife.opportunity %></td>
        <td><%= link_to 'Show', top_fife %></td>
        <td><%= link_to 'Edit', edit_top_fife_path(top_fife) %></td>
        <td><%= link_to 'Destroy', top_fife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Top five', new_top_fife_path %>
